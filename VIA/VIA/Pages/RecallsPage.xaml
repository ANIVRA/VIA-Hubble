<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VIA.Pages.RecallsPage"
             Title="ReCalls">
  
  <ContentPage.Content>
    <StackLayout>
      <Label  x:Name="infoFor" Text="Recalls for " VerticalOptions="CenterAndExpand" HorizontalOptions="Center"/>
      <Button x:Name="nextButton" Text="Next Page" VerticalOptions="CenterAndExpand" />
      <Button x:Name="previousButton" Text="Previous Page"  VerticalOptions="CenterAndExpand" />
    </StackLayout>
  </ContentPage.Content>
  
</ContentPage>